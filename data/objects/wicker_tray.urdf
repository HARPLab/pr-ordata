					<color rgba="0.6 0.6 0.6 1.0"/>
				</material>
			</geometry>
		</visual>
		<visual name="padding3">
            <origin xyz="0.225 0 0.06"/>
			<geometry>
				<box size="0.01 0.325 0.06"/>
				<material>
                    <color rgba="0.6 0.6 0.6 1.0"/>
				</material>
			</geometry>
		</visual>
		<visual name="padding4">
            <origin xyz="-0.225 0 0.06"/>
